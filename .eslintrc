{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.12.0"
    }
  },
  "rules": {
    "no-alert": "off",
    "no-console": "off",
    "no-unused-vars": "error",
    "strict": 0,
    "react/prop-types": "off"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}
